<div class="notes-new">
  <div class="notes-form">
    <div class="form-header">編集</div>
    <%= form_tag "/notes/#{@note.id}/update" do %>
      <h2>タイトル</h2>
      <input name="title" value="<%= @note.title %>">
      <h2>内容</h2>
      <textarea name="content"><%= @note.content %></textarea>
      <input type="submit" value="更新">
    <% end %>
  </div>
</div>
