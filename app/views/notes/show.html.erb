<div class="note-show">
  <div class="note">
    <img src="/images/dummy_food.png">
    <h3><%= @note.title %></h3>
    <p><%= @note.content %></p>
  </div>
  <div class="note-edit">
    <a href="/notes/<%= @note.id %>/edit">編集</a>
    <%= form_tag "/notes/#{@note.id}/destroy" do %>
      <input type="submit" value="削除">
    <% end %>
  </div>
</div>
